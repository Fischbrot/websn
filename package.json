{
  "name": "websn",
  "version": "1.2.1",
  "description": "An opinionated webpack configuration generator for traditional front-end development",
  "keywords": [
    "front-end",
    "boilerplate",
    "template",
    "generator",
    "webpack"
  ],
  "author": "Michael Serajnik <ms@mserajnik.at>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mserajnik/websn.git"
  },
  "bugs": {
    "url": "https://github.com/mserajnik/websn/issues"
  },
  "main": "generator.js",
  "bin": {
    "websn": "./generator.js"
  },
  "scripts": {
    "test": "ava --verbose"
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/test-project/**/*"
    ],
    "sources": [
      "**/*.js"
    ],
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": false,
    "compileEnhancements": true
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "fs-extra": "^6.0.1",
    "inquirer": "^5.2.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "ava": "1.0.0-beta.4",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0"
  }
}
